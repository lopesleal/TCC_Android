<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pimapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PimApp">
        <activity android:name=".TelaUsuario"></activity>
        <activity android:name=".TelaSimulacao" />
        <activity android:name=".TelaRenegociacao" />
        <activity android:name=".TelaQuitar" />
        <activity android:name=".TelaLogin" />
        <activity android:name=".TelaInicial" />
        <activity android:name=".TelaExibirMais" />
        <activity android:name=".TelaEsqueciSenha" />
        <activity android:name=".TelaCadastro" />
        <activity android:name=".TelaBoleto" />
        <activity android:name=".TelaAntecipacao" />
        <activity android:name=".TelaAjuda" />
        <activity android:name=".TelaAcompanhamento" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>